{"name":"Air Walk","type":"spell","img":"icons/svg/item-bag.svg","system":{"description":{"value":"<hr>\n<p>One character can tread on air as if walking on solid ground. He can move upward or downward at a 45-degree angle, the former at 1/2 his Speed (rounded up).</p>","chat":"","unidentified":""},"source":"FantasyCraft Core Pg. 115","school:":"none","discipline":"compass","level":"4","castingTime":{"value":1,"increment":"fantasycraft.halfAction"},"distance":{"value":"personalOrTouch","rangedAttack":false,"description":"fantasycraft.personalOrTouch"},"duration":{"value":"fixedDuration","number":"10","increment":"minute","isNotInstantOrPermanent":true,"perCastingLevel":true,"enduring":false,"dismissable":false},"save":{"saveType":"none","saveEffect":"partial","saveKeywords":{"value":["repeatable"]},"hasSave":false},"save2":{"saveType":"none","saveEffect":"partial","saveKeywords":{"value":["disbelief"]},"hasSave":false},"area":{"value":"0","shape":"none","penetrating":false,"perLevel":false,"customShape":false},"preperationCost":0,"secondaryTerms":{"value":["air"]},"damage":{"flatOrRandom":"flat","scaling":"none","diceSize":"d6","diceNum":"1","maxDamage":"12","flatDamage":0,"damageType":"fire","dealsDamage":false,"bonusDamage":0,"ap":0,"secondaryDamage":"none","secondaryDamagetype":"stress"},"school":"conjurer","preparationCost":""},"effects":[],"flags":{},"_stats":{"systemId":"fantasycraft","systemVersion":"1","coreVersion":"10.291","createdTime":1661896542798,"modifiedTime":1677523008046,"lastModifiedBy":"MbxEaKeEd7aIBbFb"},"_id":"pbOGcONLKf1ObOAQ","folder":null,"sort":0,"ownership":{"default":0,"MbxEaKeEd7aIBbFb":3}}
