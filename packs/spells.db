{"name":"Illusionary Image IV","type":"spell","img":"icons/svg/item-bag.svg","system":{"description":{"value":"<p>You create a visual illusion of any size and shape. The illusion may exhibit smell, texture, and non-damaging heat, it can move and animate, so long as it remains entirely in the Area and may include sound, including speech. The illusion may follow a script.</p>","chat":"","unidentified":""},"source":"FantasyCraft Core Pg. 133","school:":"none","discipline":"illusion","level":"4","castingTime":{"value":1,"increment":"fantasycraft.halfAction"},"distance":{"value":"remote","rangedAttack":false,"description":"fantasycraft.remote"},"duration":{"value":"fixedDuration","number":"1","increment":"minute","isNotInstantOrPermanent":true,"perCastingLevel":false,"enduring":false,"dismissable":true,"durationKeyword":true},"save":{"saveType":"will","saveEffect":"negates","saveKeywords":{"value":["disbelief"]},"hasSave":true},"save2":{"saveType":"none","saveEffect":"partial","saveKeywords":[],"hasSave":false},"area":{"value":"10","shape":"cube","penetrating":true,"perLevel":true,"customShape":false},"preperationCost":0,"secondaryTerms":{"value":[]},"damage":{"flatOrRandom":"flat","scaling":"none","diceSize":"d6","diceNum":"1","maxDamage":"12","flatDamage":0,"damageType":"fire","dealsDamage":false,"bonusDamage":0,"ap":0,"secondaryDamage":"none","secondaryDamagetype":"stress"},"school":"trickster","preparationCost":""},"effects":[],"flags":{},"_stats":{"systemId":"fantasycraft","systemVersion":"1","coreVersion":"10.291","createdTime":1661896542828,"modifiedTime":1677525102448,"lastModifiedBy":"MbxEaKeEd7aIBbFb"},"_id":"CrYEkePF7oqjwRtx","folder":null,"sort":0,"ownership":{"default":0,"MbxEaKeEd7aIBbFb":3}}
