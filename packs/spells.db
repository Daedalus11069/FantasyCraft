{"name":"Hindsight","type":"spell","img":"icons/svg/item-bag.svg","system":{"description":{"value":"<p>You project your vision and hearing back in time at your current location. You may observe events in real time or skim them at up to 60 times their normal rate (e.g. observing a minute in a second). The maximum time you may project back is 24 hours per 1 Preparation Cost.</p>","chat":"","unidentified":""},"source":"FantasyCraft Core Pg. 133","school:":"none","discipline":"diviniation","level":"7","castingTime":{"value":10,"increment":"fantasycraft.minute"},"distance":{"value":"personal","rangedAttack":false,"description":"fantasycraft.personal"},"duration":{"value":"concentrationAndDuration","number":"1","increment":"minute","isNotInstantOrPermanent":true,"perCastingLevel":false,"enduring":false,"dismissable":false,"durationText":"fantasycraft.concentrationAndDuration"},"save":{"saveType":"none","saveEffect":"partial","saveKeywords":[],"hasSave":false},"save2":{"saveType":"none","saveEffect":"partial","saveKeywords":[],"hasSave":false},"area":{"value":"20","shape":"sphere","penetrating":false,"perLevel":false,"customShape":false},"preperationCost":0,"secondaryTerms":{"value":[]},"damage":{"flatOrRandom":"flat","scaling":"none","diceSize":"d6","diceNum":"1","maxDamage":"12","flatDamage":0,"damageType":"fire","dealsDamage":false,"bonusDamage":0,"ap":0,"secondaryDamage":"none","secondaryDamagetype":"stress"},"school":"seer","preparationCost":"1"},"effects":[],"flags":{},"_stats":{"systemId":"fantasycraft","systemVersion":"1","coreVersion":"10.291","createdTime":1661896542826,"modifiedTime":1677525102434,"lastModifiedBy":"MbxEaKeEd7aIBbFb"},"_id":"B0DJEKIAofCJcqSu","folder":null,"sort":0,"ownership":{"default":0,"MbxEaKeEd7aIBbFb":3}}
