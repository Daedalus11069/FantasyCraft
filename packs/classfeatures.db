{"_id":"pYYicfuDywmUDcTc","name":"Gallantry","type":"feature","img":"icons/svg/item-bag.svg","effects":[],"folder":null,"sort":0,"flags":{},"system":{"description":{"value":"<p>At Levels 6 and 10, your Strength and Charisma scores each rise by 1.</p>","chat":"","unidentified":""},"source":"Fantasy Craft Core Pg. 57","core":false,"featureType":"class","containsFlag":false,"flag":"","number":0,"xpMultiplier":"none","damageTypes":{"string":""},"xpValue":0,"xpDifferentOnIncrease":false,"xpPerGrade":2,"grades":{"value":"","maximum":"X"},"flawless":{"isFlawless":false,"skill1":"","skill2":"","classSource":"assassin"},"numerals":false},"ownership":{"default":0,"MbxEaKeEd7aIBbFb":3},"_stats":{"systemId":"fantasycraft","systemVersion":"0.88","coreVersion":"10.291","createdTime":null,"modifiedTime":1676781523765,"lastModifiedBy":"MbxEaKeEd7aIBbFb"}}
